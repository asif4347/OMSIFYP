@model OMSIFYP.ViewModels.AdminData

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/SuperAdmin/_LayoutSuperAdmin.cshtml";
}
<p style="color:red">@ViewBag.delete</p>
<h2>List of Admins</h2>

<p>
    @Html.ActionLink("Create New", "CreateAdmin")
</p>
<table class="table" >
    <tr style="background-color:#17BAB8">
        <th>
            User ID
        </th>
        <th>
            Email
        </th>   
        <th>
           Last Name
        </th>
        <th>
            First Name
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.Admins) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.userId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstMidName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
           
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
